{
  "root": true,
  "plugins": [
    "react"
  ],
  "globals": {
    "wp": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-const-assign": 2
  },
  "parser": "babel-eslint"
}